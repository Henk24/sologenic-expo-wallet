      }
    },
    "description": "",
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "Sologenic Wallet",
  "name": "SologenicWallet",
  "extra": {
    "config": {
      "termsUrl": "https://www.sologenic.com/terms-and-conditions",
      "privacyUrl": "https://www.sologenic.com/privacy-policy",
      "contactUsUrl": "https://www.coinfield.com/company/contactus"
    }
  }
}
